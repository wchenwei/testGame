        <log.logback>1.2.3</log.logback>
        <log.slf4j-api>1.7.30</log.slf4j-api>
        <sun-tools.version>1.8</sun-tools.version>
        <ikanalyzer.version>2012_u6</ikanalyzer.version>
        <fastjson.version>1.2.41</fastjson.version>
        <hg.core>1.0.1</hg.core>
        <hg.mq.version>1.3.3</hg.mq.version>
        <json.version>20170516</json.version>
        <apache.commons.version>2.6</apache.commons.version>

        <game-engine.lib-ai>1.0.9</game-engine.lib-ai>
