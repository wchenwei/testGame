        <project.reporting.outputEncoding>UTF-8</project.reporting.outputEncoding>
        <java.version>1.8</java.version>
        <mavenPlugin.version>3.1</mavenPlugin.version>
        <apollo.version>1.4.0</apollo.version>
        <org.springframework.version>5.0.2.RELEASE</org.springframework.version>
        <spring-data-mongodb>2.0.7.RELEASE</spring-data-mongodb>
        <netty.version>4.1.63.Final</netty.version>
        <protobuf.version>2.5.0</protobuf.version>
        <aliyun.ons.client.version>1.8.4.Final</aliyun.ons.client.version>
        <aliyun.oss.version>3.10.2</aliyun.oss.version>
        <aliyun.log.version>0.6.16</aliyun.log.version>
        <aliyun.producer.version>0.1.11</aliyun.producer.version>
